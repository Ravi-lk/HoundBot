# HoundBot Configuration
# Copy this file to .env and fill in your values

# Ollama API Key (get from https://ollama.com/settings/keys)
OLLAMA_API_KEY=your_api_key_here

# Ollama Cloud Model (use qwen3.5:397b for the cloud version)
OLLAMA_MODEL=qwen3.5:397b

# Ollama API Base URL (for cloud models use https://ollama.com)
OLLAMA_BASE_URL=https://ollama.com
